{
  "name": "food-delivery-backend",
  "version": "1.0.0",
  "description": "Production-ready multi-restaurant food delivery backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node src/db/migrate.js",
    "migrate": "node scripts/migrate.js",
    "migrate:dry": "node scripts/migrate.js --dry",
    "migrate:drizzle": "drizzle-kit generate && drizzle-kit migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.30.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "google-auth-library": "^9.10.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "razorpay": "^2.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "drizzle-kit": "^0.21.4",
    "nodemon": "^3.1.2"
  }
}
